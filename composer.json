{
    "name": "sanjaya/laravel-daisyui-starter",
    "description": "A Laravel starter kit with DaisyUI integration",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Sanjaya",
            "email": "sanjayaprasanna20@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/breeze": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sanjaya\\LaravelDaisyuiStarter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sanjaya\\LaravelDaisyuiStarter\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sanjaya\\LaravelDaisyuiStarter\\LaravelDaisyuiStarterServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/sanjayacloud/laravel-daisyui-starter"
        }
    ],
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}